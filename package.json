{
  "name": "top_ramen",
  "version": "1.0.0",
  "main": "server/server.js",
  "scripts": {
    "lint": "eslint **",
    "pretest": "eslint .",
    "start": "pm2 start $npm_package_main -i max --name=$npm_package_name --log-date-format='MM.DD.YY@HH:mm:SSS'",
    "start-dev": "pm2 start $npm_package_main --name=$npm_package_name --log-date-format='MM.DD.YY@HH:mm:SSS'",
    "stop": "pm2 stop $npm_package_name",
    "restart": "pm2 restart $npm_package_name --log-date-format='MM.DD.YY@HH:mm:SSS'",
    "status": "pm2 show $npm_package_name"
  },
  "config": {
    "logDir": "/var/log/nodejs/top-ramen/"
  },
  "dependencies": {
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "helmet": "^1.3.0",
    "lodash": "4.16.4",
    "loopback": "^2.22.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^2.4.0",
    "loopback-component-passport": "^2.1.0",
    "loopback-connector-mongodb": "^1.15.2",
    "loopback-datasource-juggler": "^2.39.0",
    "memcached": "2.2.2",
    "serve-favicon": "^2.0.1",
    "strong-cluster-connect-store": "^1.0.3",
    "strong-error-handler": "^1.0.1",
    "strong-store-cluster": "^1.0.1"
  },
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "eslint": "^2.13.1",
    "eslint-config-loopback": "^4.0.0",
    "nsp": "^2.1.0"
  },
  "repository": {
    "type": "",
    "url": "https://github.com/toekneefof/top_ramen_server"
  },
  "license": "UNLICENSED",
  "description": "Top Ramen Server"
}
